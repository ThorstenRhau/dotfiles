	file-style = "#575279"
	hunk-header-decoration-style = "#9893a5" box ul
	hunk-header-file-style = bold
	hunk-header-line-number-style = bold "#797593"
	hunk-header-style = file line-number syntax
	line-numbers-left-style = "#9893a5"
	line-numbers-minus-style = bold "#b4637a"
	line-numbers-plus-style = bold "#56949f"
	line-numbers-right-style = "#9893a5"
	line-numbers-zero-style = "#797593"
	minus-style = syntax "#f4ede8"
	minus-non-emph-style = syntax "#f4ede8"
	minus-emph-style = bold syntax "#ede3de"
	minus-empty-line-marker-style = syntax "#f4ede8"
	plus-style = syntax "#eef5f3"
	plus-non-emph-style = syntax "#eef5f3"
	plus-emph-style = bold syntax "#e4eeec"
	plus-empty-line-marker-style = syntax "#eef5f3"
	map-styles = \
		bold purple => syntax "#efebe9", \
		bold blue => syntax "#e8eff5", \
		bold cyan => syntax "#e8f0f2", \
		bold yellow => syntax "#f5f0e8"
	# Should match the name of the bat theme
	syntax-theme = rose_pine_dawn
