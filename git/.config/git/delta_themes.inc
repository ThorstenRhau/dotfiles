[delta "rose-pine-moon"]
	blame-palette = "#232136 #2a273f #393552 #6e6a86 #908caa"
	commit-decoration-style = "#6e6a86" bold box ul
	dark = true
	file-decoration-style = "#6e6a86"
	file-style = "#e0def4"
	hunk-header-decoration-style = "#6e6a86" box ul
	hunk-header-file-style = bold
	hunk-header-line-number-style = bold "#908caa"
	hunk-header-style = file line-number syntax
	line-numbers-left-style = "#6e6a86"
	line-numbers-minus-style = bold "#eb6f92"
	line-numbers-plus-style = bold "#9ccfd8"
	line-numbers-right-style = "#6e6a86"
	line-numbers-zero-style = "#908caa"
	minus-style = syntax "#2d1f2f"
	minus-non-emph-style = syntax "#2d1f2f"
	minus-emph-style = bold syntax "#3d2839"
	minus-empty-line-marker-style = syntax "#2d1f2f"
	plus-style = syntax "#233b3a"
	plus-non-emph-style = syntax "#233b3a"
	plus-emph-style = bold syntax "#2d4b49"
	plus-empty-line-marker-style = syntax "#233b3a"
	map-styles = \
		bold purple => syntax "#3e3852", \
		bold blue => syntax "#32455a", \
		bold cyan => syntax "#334a4d", \
		bold yellow => syntax "#4a4136"
	# Should match the name of the bat theme
	syntax-theme = rose_pine_moon

